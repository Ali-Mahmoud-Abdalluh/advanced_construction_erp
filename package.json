{
	"private": true,
	"name": "advanced_construction_erp",
	"description": "Open Source Advanced Construction ERP",
	"repository": {
	  "type": "git",
	  "url": "git+https://github.com/Ali-Mahmoud-Abdalluh/advanced_construction_erp.git"
	},
	"homepage": "https://github.com/Ali-Mahmoud-Abdalluh/advanced_construction_erp",
